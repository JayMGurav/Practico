{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "allowImportExportEverywhere": true,
        "codeFrame": true
    },
    "rules": {
        "semi": 2,
        "prettier/prettier": "error"
    },
    "extends": ["prettier", "plugin:prettier/recommended"],
    "plugins": ["prettier"]
}
